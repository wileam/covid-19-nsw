{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf build && yarn run react:build && yarn run ship",
    "ship": "cp -rf build build.bak && gzip -r build && rsync -avhu --progress ./build.bak/ ./build/ && rm -rf build.bak",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react:build": "GENERATE_SOURCEMAP=false react-scripts build",
    "screenshot": "node screenshot.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "echarts": "^4.6.0",
    "echarts-for-react": "^2.0.15-beta.1",
    "ejs": "^3.0.1",
    "husky": ">=4",
    "lint-staged": ">=10",
    "lodash.uniq": "^4.5.0",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.28",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-iframe": "^1.8.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-social-sharing": "^3.0.1",
    "react-twitter-embed": "^3.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{jsx, js,css,md}": "prettier --write"
  },
  "dependencies": {
    "ag-grid-community": "^23.0.2",
    "ag-grid-react": "^23.0.2",
    "lodash.uniqby": "^4.7.0"
  }
}
